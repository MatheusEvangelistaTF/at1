#include <stdio.h>

int main(){
    int numeros[6];
    int i;

    printf("Digite 6 números para o sorteio:\n");

    for (i = 0; i < 6; i++){
        printf("Número %d: ", i + 1);
        scanf("%d", &numeros[i]);
    }

    printf("\nNúmeros escolhidos para o sorteio:\n");
    for (i = 0; i < 6; i++){
        printf("%d ", numeros[i]);
    }

    printf("\n");

    return 0;
}
